{
	"applications": {
		"gecko": {
			"id": "copytextlink_dev@zatsunenomokou.eu",
			"update_url": "https://gitlab.com/ZatsuneNoMokou/copy_text_link/raw/master/dist/copytextlink_dev.update.json",
			"strict_min_version": "50.0"
		}
	},
	"manifest_version": 2,
	"name": "Copy Text Link (Dev)",
	"short_name": "CopyTextLinkDev",
	"description": "__MSG_extShortDesc__",
	"author": "ZatsuneNoMokou",
	"version": "1.0.0",
	"icons": {
		"96": "/icon_96.png",
		"128": "/icon.png"
	},
	"background": {
		"scripts": [ "index.js" ]
	},
	"browser_action": {
		"default_icon": {
			"16": "/icon_16.png",
			"32": "/icon_32.png",
			"48": "/icon_48.png",
			"64": "/icon_64.png",
			"96": "/icon_96.png",
			"128": "/icon.png"
		}
	},
	"content_scripts": [
		{
			"js": [ "data/js/page_contentScript.js" ],
			"matches": [ "http://*/*", "https://*/*" ],
			"run_at": "document_end"
		}
	],
	"default_locale": "en",
	"offline_enabled": false,
	"permissions": [
		"tabs",
		"activeTab",
		"contextMenus",
		"clipboardWrite",
		"notifications",
		"http://*/*",
		"https://*/*"
	],
	"minimum_chrome_version": "42.0",
	"web_accessible_resources": [ "/icon.png" ]
}
